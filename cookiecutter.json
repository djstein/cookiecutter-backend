    "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_') }}",
  "description": "Django Zappa on AWS Lambda",
  "author_name": "Dylan Stein",
  "domain_name": "example.com",
  "github_user": "djstein",
  "email": "{{ cookiecutter.author_name.lower()|replace(' ', '-') }}@ncsu.edu",
  "version": "0.1.0",
  "open_source_license": [
    "MIT",
    "BSD",
    "GPLv3",
    "Apache Software License 2.0",
    "Not open source"
  ],
  "timezone": "UTC",
  "postgresql_version": ["10.4", "10.3", "10.2", "10.1", "9.6"],
  "aws_region": "us-east-1",
